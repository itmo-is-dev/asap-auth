{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.Extensions.Http": "Information",
        "Grpc.AspNetCore.Server.Model.Internal": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:T} [{Level:u3}] {SourceContext} {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Serilogs/asap-auth/AppLogs_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] {SourceContext} {Message}{NewLine}{Exception}",
          "retainedFileCountLimit": 30
        }
      }
    ]
  },
  "Sentry": {
    "Enabled": true,
    "Dsn": "",
    "Environment": "",
    "Debug": false,
    "TracesSampleRate": 1.0,
    "MinimumEventLevel": "Warning"
  },
  "Platform": {
    "Environment": "",
    "YandexCloud": {
      "ServiceUri": "",
      "LockBox": {
        "SecretId": ""
      }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Infrastructure": {
    "Identity": {
      "Configuration": {
        "Secret": "",
        "Issuer": "",
        "Audience": "",
        "ExpiresMinutes": 120
      }
    },
    "PostgresConfiguration": {
      "Host": "",
      "Database": "",
      "Port": 0,
      "Username": "",
      "Password": "",
      "SslMode": "Prefer"
    }
  },
  "Startup": {
    "DefaultAdmins": [
    ]
  }
}
